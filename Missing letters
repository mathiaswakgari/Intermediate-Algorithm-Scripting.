function fearNotLetter(str) {

  let strArr = []
  
  for(let i=0;i < str.length; i++){
    strArr.push(str.charCodeAt(i))
  }
  
  for(let j = 0; j < strArr.length; j++ ){
    if(strArr[j + 1] - strArr[j] === 2){
      return String.fromCharCode(strArr[j] + 1)
    }
  }


  
  
}

fearNotLetter("abce");
